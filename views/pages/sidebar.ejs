<div class="container-fluid">
  <div class="row">
    <div class="sidebar-wrapper it-line-left-side col-lg-3 col-md-4">
      <div class="sidebar-wrapper">
        <div class="list-wrapper">
          <p></p>
          <h3 class="mb-4">Trova Il Tuo Film</h3>

          <div class="form-group">
            <label for="genere" class="form-label">Genere</label>
            <select id="genere" class="form-select">
              <option selected disabled></option>
              <% generi.forEach((genere)=> { %>
                <option value="<%=genere %>">
                  <%= genere.genere %>
                </option>
                <% }); %>
            </select>
          </div>

          <div class="form-group">
            <label for="anno" class="form-label">Anno</label>
            <select id="anno" class="form-select">
              <option selected disabled></option>
              <% anni.forEach((anno)=> { %>
                <option value="<%=anno %>">
                  <%= anno.anno %>
                </option>
                <% }); %>
            </select>
          </div>

          <button class="btn btn-outline btn-custom w-100 mt-4" id="applicaFiltri">Applica filtri</button>
          <p></p>
        </div>
      </div>
    </div>